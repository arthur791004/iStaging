{
  "name": "istaging-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://arthur791004.github.io/iStaging/",
  "devDependencies": {
    "@17media/eslint-config-17media": "~2.0.0",
    "@babel/core": "~7.2.2",
    "@babel/plugin-proposal-class-properties": "~7.2.3",
    "@babel/plugin-proposal-decorators": "~7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "~7.2.0",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/plugin-transform-async-to-generator": "~7.2.0",
    "@babel/preset-env": "~7.2.3",
    "@babel/preset-react": "~7.0.0",
    "@rescripts/cli": "~0.0.7",
    "@rescripts/rescript-env": "~0.0.3",
    "cross-env": "~5.2.0",
    "gh-pages": "~2.0.1",
    "husky": "~1.3.1",
    "lint-staged": "~8.1.0",
    "react-scripts": "2.1.3",
    "rimraf": "~2.6.3"
  },
  "dependencies": {
    "react": "~16.7.0",
    "react-dom": "~16.7.0"
  },
  "scripts": {
    "preinstall": "unlink ./node_modules/@",
    "postinstall": "ln -sf ../src ./node_modules/@",
    "start": "cross-env NODE_ENV=development rescripts start",
    "build": "cross-env NODE_ENV=production rescripts build",
    "test": "rescripts test",
    "deploy": "gh-pages -d build",
    "clean": "rimraf build",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "yarn run lint:eslint"
  }
}
